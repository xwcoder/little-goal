{
  "compilerOptions": {
    "outDir": "./docs",
    "module": "es6",
    "moduleResolution": "Node",
    "target": "es2017",
    "rootDir": "src",
    "removeComments": false,
    "preserveConstEnums": true,
    "sourceMap": false,
    "noEmitOnError": true,
    "experimentalDecorators": true,
    "declaration": false,
    "lib": ["ES2017", "WebWorker"]
  },
  "include": [
    "src/sw.ts"
  ],
  "exclude": [
    "node_modules",
    "**/*.test.ts",
    "**/*.spec.ts",
    "**/*.d.ts",
    "**/test/**/*.ts"
  ]
}
